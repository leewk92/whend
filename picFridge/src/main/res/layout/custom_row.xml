<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
 
        <LinearLayout
            android:id="@+id/back"
            style="@style/MyListBackContent"
            android:background="@android:color/transparent"
            android:tag="back" >
 
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/swipe_button1"
                style="@style/MyListButtonAction"
                android:text="Button 1"/>
 
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/swipe_button2"
                style="@style/MyListButtonAction"
                android:text="Button 2"/>
 
        <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/swipe_button3"
                style="@style/MyListButtonAction"
                android:text="Button 3"/>
 
    </LinearLayout>
     
     <RelativeLayout
         android:id="@+id/front"
         style="@style/MyListFrontContent"
         android:background="@android:color/transparent"
         android:orientation="vertical"
         android:tag="front" >
         
     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         android:background="@drawable/background_with_shadow"
         android:orientation="vertical" >

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="fill_horizontal"
            android:orientation="vertical"
            android:padding="5dp" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center" >

                <ImageView
                    android:id="@+id/company_image"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_weight="0.5"
                    android:padding="8dp"
                    android:src="@drawable/icon_whend" />

                <TextView
                    android:id="@+id/feed_company"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_weight="90"
                    android:gravity="center_vertical"
                    android:padding="8dp"
                    android:text="company_name" />

                <TextView
                    android:id="@+id/num_following"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="Button" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="2dp"
                    android:layout_weight="0.5"
                    android:orientation="vertical" >

          <!--      <Button
                        android:id="@+id/feed_remove_wall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_marginTop="5dp"
                        android:layout_weight="0.5"
                        android:gravity="center_vertical|center_horizontal|center"
                        android:text="피드 보지 않기"
                        android:textSize="13sp" />

                    <Button
                        android:id="@+id/feed_incalendar"
                        style="?android:attr/buttonStyleSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical|center_horizontal"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="0.5"
                        android:text="캘린더에 넣기"
                        android:textSize="13sp" />
			-->
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="80" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="200"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/feed_item"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="80"
                            android:paddingLeft="13dp"
                            android:paddingTop="10dp"
                            android:text="행사"
                            android:textSize="17sp" />

                        <TextView
                            android:id="@+id/feed_startdate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="80"
                            android:paddingLeft="13dp"
                            android:paddingTop="25dp"
                            android:text="시작 시간"
                            android:textSize="13sp" />
                        <TextView
                        	android:id="@+id/duration"
	                        android:layout_width="wrap_content"
	                        android:layout_height="wrap_content"
	                        android:layout_weight="80"
	                        android:paddingLeft="50dp"
	                        android:paddingTop="0dp"
	                        android:text="~"
	                    />
                         
                        <TextView
                            android:id="@+id/feed_enddate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="80"
                            android:paddingLeft="13dp"
                            android:paddingTop="5dp"
                            android:text="끝나는 시간"
                            android:textSize="13sp" />
                        

                        <TextView
                            android:id="@+id/feed_place"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:layout_weight="80"
                            android:paddingLeft="13dp"
                            android:paddingTop="10dp"
                            android:text="장소"
                            android:textSize="13sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="70"
                        android:orientation="vertical" >

                        <ImageView
                            android:id="@+id/feed_image"
                            android:layout_weight="50"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/calfeed_default_image" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

    </GridLayout>

</LinearLayout>
         
         
         
         
         
         
         
         
         
         
        
 
    </RelativeLayout>
 
</FrameLayout>